year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

citEntry(entry = "Manual",
    title = "{rStar}: {P}ackage for Holsten-Laubach-Williams Natural Rate of           Interest Estimation",
    author = person("Jannes", "Reddig")
    note = note,
    year = year,
    url = "https://github.com/JannesRed/rStar")
)





